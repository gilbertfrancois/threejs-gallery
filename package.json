{
    "name": "react-threejs-template",
    "version": "1.0.0",
    "description": "React - Three.js template",
    "scripts": {
        "dev": "npx parcel --host 0.0.0.0 --port 42347 src/index.html",
        "prod": "npx parcel build src/index.html",
        "clean": "rm -rf dist; rm -rf node_modules; rm -rf .parcel-cache; rm package-lock.json",
        "cp": "cp -r ./assets ./dist/",
        "format": "prettier --write \"**/*.{html,js,jsx,json,ts,tsx}\""
    },
    "author": "Gilbert Francois Duivesteijn",
    "license": "Apache-2.0",
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "three": "^0.145.0",
        "@tweenjs/tween.js": "^18.6.4"
    },
    "devDependencies": {
        "@parcel/transformer-glsl": "^2.7.0",
        "parcel": "^2.7.0",
        "prettier": "^2.7.1",
        "process": "^0.11.10"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
